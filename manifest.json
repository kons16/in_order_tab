{
    "manifest_version": 2,
    "name": "In Order Tab",
    "version": "1.0",
  
    "background": {
      "scripts": ["background.js"]
    },
    "icons": {
        "48": "simple.png"
    },
    "browser_action": {
        "default_icon": "simple.png",
        "default_title": "In Order Tab"
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "background",
        "http://*/*",
        "https://*/*"
      ]
  }